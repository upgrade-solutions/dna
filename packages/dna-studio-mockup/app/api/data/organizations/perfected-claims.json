  "resourceSchemas": {
    "Case": {
      "attributes": [
        { "name": "caseTitle", "type": "String", "required": true },
        { "name": "caseType", "type": "String", "required": true },
        { "name": "defendantCorporation", "type": "String", "required": true },
        { "name": "harmDescription", "type": "String", "required": true },
        { "name": "caseStatus", "type": "String", "required": true },
        { "name": "filingDate", "type": "Date", "required": false },
        { "name": "estimatedVictims", "type": "Number", "required": false },
        { "name": "settlementAmount", "type": "Number", "required": false },
        { "name": "primaryAttorney", "type": "String", "required": false },
        { "name": "legalPartnerFirm", "type": "String", "required": false }
      ],
      "validations": [
        {
          "field": "caseTitle",
          "rules": ["Min Length: 5", "Max Length: 200"],
          "description": "Clear, descriptive case title"
        },
        {
          "field": "caseType",
          "rules": ["Values: Product Liability, Environmental, Pharmaceutical, Medical Device, Workplace Safety, Consumer Protection"],
          "description": "Must be a recognized mass tort category"
        },
        {
          "field": "caseStatus",
          "rules": ["Values: Investigation, Evidence Gathering, Filed, Discovery, Settlement Negotiations, Resolved, Dismissed"],
          "description": "Current status of the case"
        },
        {
          "field": "harmDescription",
          "rules": ["Min Length: 50", "Max Length: 2000"],
          "description": "Detailed description of the harm caused"
        }
      ]
    },
    "Client": {
      "attributes": [
        { "name": "firstName", "type": "String", "required": true },
        { "name": "lastName", "type": "String", "required": true },
        { "name": "email", "type": "String", "required": true },
        { "name": "phoneNumber", "type": "String", "required": true },
        { "name": "dateOfBirth", "type": "Date", "required": true },
        { "name": "address", "type": "Object", "required": true },
        { "name": "caseIds", "type": "String[]", "required": false },
        { "name": "representativeType", "type": "String", "required": true },
        { "name": "medicalRecordsProvided", "type": "Boolean", "required": false },
        { "name": "consentToContact", "type": "Boolean", "required": true },
        { "name": "intakeDate", "type": "DateTime", "required": true }
      ],
      "validations": [
        {
          "field": "email",
          "rules": ["Format: Email", "Max Length: 255"],
          "description": "Valid email address for client communication"
        },
        {
          "field": "phoneNumber",
          "rules": ["Format: E.164", "Length: 10-15"],
          "description": "Valid phone number for client contact"
        },
        {
          "field": "representativeType",
          "rules": ["Values: Self, Representative"],
          "description": "Whether filing for themselves or someone else"
        },
        {
          "field": "dateOfBirth",
          "rules": ["Max Age: 120", "Format: ISO 8601"],
          "description": "Valid birth date for age verification"
        }
      ]
    },
    "Claimant": {
      "attributes": [
        { "name": "clientId", "type": "String", "required": true },
        { "name": "caseId", "type": "String", "required": true },
        { "name": "exposureDetails", "type": "Object", "required": true },
        { "name": "healthConditions", "type": "String[]", "required": true },
        { "name": "diagnosisDate", "type": "Date", "required": false },
        { "name": "treatmentHistory", "type": "String", "required": false },
        { "name": "economicDamages", "type": "Object", "required": false },
        { "name": "eligibilityStatus", "type": "String", "required": true },
        { "name": "documentationScore", "type": "Number", "required": false },
        { "name": "assignedLawFirm", "type": "String", "required": false }
      ],
      "validations": [
        {
          "field": "exposureDetails",
          "rules": ["Required Fields: product, duration, frequency, dateRange"],
          "description": "Detailed information about exposure to harmful product/substance"
        },
        {
          "field": "healthConditions",
          "rules": ["Min Items: 1", "Max Items: 20"],
          "description": "List of health conditions related to the exposure"
        },
        {
          "field": "eligibilityStatus",
          "rules": ["Values: Pending Review, Eligible, Ineligible, Needs Documentation, Approved"],
          "description": "Current status of claimant eligibility"
        },
        {
          "field": "documentationScore",
          "rules": ["Min: 0", "Max: 100"],
          "description": "Score based on completeness and quality of documentation"
        }
      ]
    },
    "Evidence Files": {
      "attributes": [
        { "name": "fileType", "type": "String", "required": true },
        { "name": "fileName", "type": "String", "required": true },
        { "name": "fileSize", "type": "Number", "required": true },
        { "name": "caseId", "type": "String", "required": true },
        { "name": "uploadedBy", "type": "String", "required": true },
        { "name": "uploadDate", "type": "DateTime", "required": true },
        { "name": "evidenceType", "type": "String", "required": true },
        { "name": "confidentialityLevel", "type": "String", "required": true },
        { "name": "verificationStatus", "type": "String", "required": true }
      ],
      "validations": [
        {
          "field": "fileType",
          "rules": ["Values: PDF, DOCX, JPG, PNG, MP4, MP3"],
          "description": "Supported file formats for evidence"
        },
        {
          "field": "evidenceType",
          "rules": ["Values: Medical Records, Corporate Documents, Expert Report, Witness Statement, Product Documentation, Test Results"],
          "description": "Category of evidence being submitted"
        },
        {
          "field": "confidentialityLevel",
          "rules": ["Values: Public, Confidential, Attorney Eyes Only, Highly Confidential"],
          "description": "Level of confidentiality required"
        },
        {
          "field": "fileSize",
          "rules": ["Max: 50MB"],
          "description": "Maximum file size limit"
        }
      ]
    },
    "Medical Records": {
      "attributes": [
        { "name": "patientName", "type": "String", "required": true },
        { "name": "medicalProvider", "type": "String", "required": true },
        { "name": "recordType", "type": "String", "required": true },
        { "name": "treatmentDate", "type": "Date", "required": true },
        { "name": "diagnosis", "type": "String", "required": true },
        { "name": "symptoms", "type": "String[]", "required": false },
        { "name": "treatmentPlan", "type": "String", "required": false },
        { "name": "medications", "type": "String[]", "required": false },
        { "name": "relatedToCase", "type": "Boolean", "required": true }
      ],
      "validations": [
        {
          "field": "recordType",
          "rules": ["Values: Diagnosis Report, Treatment Summary, Lab Results, Imaging Study, Surgical Report, Pathology Report"],
          "description": "Type of medical record being submitted"
        },
        {
          "field": "patientName",
          "rules": ["Min Length: 2", "Max Length: 100"],
          "description": "Full legal name of the patient"
        },
        {
          "field": "diagnosis",
          "rules": ["Min Length: 5", "Max Length: 500"],
          "description": "Medical diagnosis related to the case"
        }
      ]
    },
    "Intake Form": {
      "attributes": [
        { "name": "contactInformation", "type": "Object", "required": true },
        { "name": "exposureInformation", "type": "Object", "required": true },
        { "name": "healthInformation", "type": "Object", "required": true },
        { "name": "legalRepresentation", "type": "Object", "required": false },
        { "name": "consentAgreements", "type": "Object", "required": true },
        { "name": "referralSource", "type": "String", "required": false },
        { "name": "urgencyLevel", "type": "String", "required": true },
        { "name": "submissionDate", "type": "DateTime", "required": true }
      ],
      "validations": [
        {
          "field": "contactInformation",
          "rules": ["Required Fields: name, email, phone, address"],
          "description": "Complete contact details for client communication"
        },
        {
          "field": "exposureInformation",
          "rules": ["Required Fields: product, startDate, endDate, frequency, circumstances"],
          "description": "Detailed exposure history to harmful product/substance"
        },
        {
          "field": "healthInformation",
          "rules": ["Required Fields: currentConditions, diagnosisDate, treatingPhysician"],
          "description": "Current health status and medical history"
        },
        {
          "field": "urgencyLevel",
          "rules": ["Values: Low, Medium, High, Critical"],
          "description": "Priority level for case processing"
        }
      ]
    }
  }
}