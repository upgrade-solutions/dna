  "resourceSchemas": {
    "Account": {
      "attributes": [
        { "name": "companyName", "type": "String", "required": true },
        { "name": "accountType", "type": "String", "required": true },
        { "name": "taxId", "type": "String", "required": true },
        { "name": "businessAddress", "type": "Object", "required": true },
        { "name": "billingAddress", "type": "Object", "required": false },
        { "name": "primaryContact", "type": "Object", "required": true }
      ],
      "validations": [
        {
          "field": "companyName",
          "rules": ["Min Length: 2", "Max Length: 200"],
          "description": "Valid company or publisher name"
        },
        {
          "field": "accountType",
          "rules": ["Values: Publisher, Distributor, Author"],
          "description": "Account must be one of the valid types"
        },
        {
          "field": "taxId",
          "rules": ["Format: EIN or SSN", "Length: 9-11"],
          "description": "Valid tax identification number"
        }
      ]
    },
    "Contract": {
      "attributes": [
        { "name": "contractType", "type": "String", "required": true },
        { "name": "parties", "type": "Object[]", "required": true },
        { "name": "terms", "type": "Object", "required": true },
        { "name": "effectiveDate", "type": "Date", "required": true },
        { "name": "expirationDate", "type": "Date", "required": false },
        { "name": "status", "type": "String", "required": true }
      ],
      "validations": [
        {
          "field": "contractType",
          "rules": ["Values: Distribution, Publishing, Author"],
          "description": "Must be a valid contract type"
        },
        {
          "field": "effectiveDate",
          "rules": ["Format: ISO 8601", "Must be: Future or Present"],
          "description": "Contract effective date"
        }
      ]
    },
    "Project": {
      "attributes": [
        { "name": "title", "type": "String", "required": true },
        { "name": "description", "type": "String", "required": false },
        { "name": "authorId", "type": "String", "required": true },
        { "name": "genre", "type": "String", "required": true },
        { "name": "targetAudience", "type": "String", "required": true },
        { "name": "estimatedLength", "type": "Number", "required": false }
      ],
      "validations": [
        {
          "field": "title",
          "rules": ["Min Length: 1", "Max Length: 500"],
          "description": "Project title"
        },
        {
          "field": "genre",
          "rules": ["Values: Fiction, Non-Fiction, Biography, Self-Help, Business, etc."],
          "description": "Content genre classification"
        }
      ]
    },
    "T&Cs": {
      "attributes": [
        { "name": "version", "type": "String", "required": true },
        { "name": "acceptanceDate", "type": "DateTime", "required": true },
        { "name": "ipAddress", "type": "String", "required": true },
        { "name": "userAgent", "type": "String", "required": true }
      ],
      "validations": [
        {
          "field": "version",
          "rules": ["Format: Semantic Version"],
          "description": "Terms and conditions version"
        }
      ]
    }
  }
}